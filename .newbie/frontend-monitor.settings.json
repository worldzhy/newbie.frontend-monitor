{
  "config-service": {
    "frontend-monitor": {
      "isLocalDev": false,
      "adminWhiteList": "process.env.FRONTEND_MONITOR_ADMIN_WHITE_LIST ||'http://localhost:3011,http://localhost:8080'",
      "port": "Number(process.env.FRONTEND_MONITOR_APP_PORT || 7002)",
      "clickhouse": "process.env.FRONTEND_MONITOR_CLICKHOUSE ||'localhost:8123:default::web_monitor:'",
      "mongoose": {
        "uri": "process.env.FRONTEND_MONITOR_MONGO_URL|| 'mongodb://127.0.0.1:27017/web-monitor'"
      },
      "redis": "process.env.FRONTEND_MONITOR_REDIS || '127.0.0.1:6379::1'",
      "redis_consumption": {
        "task_time": "*/10 * * * * *",
        "thread_web": 1000,
        "thread_wx": 1000,
        "total_limit_web": 100000,
        "total_limit_wx": 100000
      },
      "TENCENTMAPKEY": "process.env.FRONTEND_MONITOR_TENCENT_MAP_KEY || ''",
      "top_alalysis_size": {
        "web": 10,
        "wx": 10
      },
      "pvuvip_task_minute_cron_time": "0 */2 * * * *",
      "pvuvip_task_minute_lock_time": 20,
      "ipRedisTTL": 2592000,
      "ip_task_cron_time": "0 */1 * * * *",
      "ip_task_lock_time": 20,
      "ip_task_space_time": 60000,
      "alarm": false,
      "alarm_task_cron_time": "0 */1 * * * *",
      "alarm_task_lock_time": 20,
      "alarm_time": 60000,
      "is_web_consume_task_run": true,
      "is_wx_consume_task_run": true,
      "day_report_num": true,
      "day_report_num_task_cron_time": "0 0 0 */1 * *",
      "day_report_num_task_lock_time": 3000,
      "pvuvip_task_day_time": "0 0 0 */1 * *",
      "day_report": false,
      "day_report_task_cron_time": "0 0 0 */1 * *",
      "day_report_task_lock_time": 3000,
      "ip_thread": 5,
      "appid_task_time": "0 */5 * * * *",
      "pageSize": 15
    }
  },
  "env": {
    "FRONTEND_MONITOR_ADMIN_WHITE_LIST": "",
    "FRONTEND_MONITOR_APP_PORT": "",
    "FRONTEND_MONITOR_MONGO_URL": "",
    "FRONTEND_MONITOR_CLICKHOUSE": "",
    "FRONTEND_MONITOR_REDIS": "",
    "FRONTEND_MONITOR_TENCENT_MAP_KEY": ""
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/mongoose": "^10.0.1",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^3.0.1",
    "clickhouse-orm": "^3.0.0-alpha5",
    "cron": "^2.4.4",
    "cron-parser": "^4.6.0",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "ioredis": "^5.4.1",
    "ip": "^2.0.1",
    "moment": "^2.29.4",
    "mongoose": "^7.6.3",
    "nodemailer": "^6.9.13",
    "ua-parser-js": "^1.0.36"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.2",
    "@types/node": "^20.11.10",
    "typescript": "^5.6.3"
  }
}